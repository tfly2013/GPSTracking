<h1>Listing Api Locations</h1>

<table>
  <thead>
    <tr>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Accuracy</th>
      <th>Speed</th>
      <th>Time</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_locations.each do |api_location| %>
      <tr>
        <td><%= api_location.latitude %></td>
        <td><%= api_location.longitude %></td>
        <td><%= api_location.accuracy %></td>
        <td><%= api_location.speed %></td>
        <td><%= api_location.time %></td>
        <td><%= api_location.user %></td>
        <td><%= link_to 'Show', api_location %></td>
        <td><%= link_to 'Edit', edit_api_location_path(api_location) %></td>
        <td><%= link_to 'Destroy', api_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location', new_api_location_path %>
