<!DOCTYPE html>
<html>
<head>
	<title>GPSTrackingOrangeWeb</title>
	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyA-10-w06yl2bTDNkIPGT0sD52X32pAyZE", :type => "text/javascript" %>
	<%= javascript_include_tag "https://google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.1.2/src/markerclusterer_packed.js", :type => "text/javascript" %>	
	<%= csrf_meta_tags %>
</head>
<body>
	<div>
		<% if user_signed_in? %>
		<%= link_to'Sign Out', destroy_user_session_path, :method => :delete %>
		<br />
		<%= link_to 'Edit registration', edit_user_registration_path %>
		<% else %>
		<%= link_to 'Sign In', new_user_session_path  %>
		<br />
		<%= link_to 'Register', new_user_registration_path  %>
		<% end %>
	</div>
	<p class="notice" id="notice">
		<%= notice %>
	</p>
	<p class="alert" id="alert">
		<%= alert %>
	</p>
	<%= yield %>
</body>
</html>
